{
    "name": "tyaunen/ayutenn-core",
    "description": "ayutennは、御茶請稔によるオレオレフレームワークです。第三者からの利用は想定していません。",
    "version": "1.0.0",
    "type": "library",
    "license": "CC-BY-1.0",
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "ayutenn\\core\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "./tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/migrate.php"
    ]
}
